    "%%writefile time_func.py\n",
    "import time\n",
    "\n",
    "def time_func(func):\n",
    "    def wrapper(*args, **kwargs):\n",
    "        ti = time.time()\n",
    "        func(*args, **kwargs)\n",
    "        tf = time.time()\n",
    "        print(\"%s running time: %f sec\"%(func.__name__, tf-ti))\n",
    "    \n",
    "    return wrapper"
   ]
